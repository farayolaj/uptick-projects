<a href="/rooms/{{id}}">
  <li
    class="flex justify-between gap-x-6 py-2 px-2 hover:bg-slate-200 border rounded-md"
  >
    <div class="min-w-0 flex-auto">
      <p class="text-sm font-semibold leading-6 text-gray-900">{{name}}</p>
      <p class="mt-1 truncate text-xs leading-5 text-gray-500">Created By:
        {{createdBy.firstName}}
        {{createdBy.lastName}}</p>
    </div>
  </li>
</a>